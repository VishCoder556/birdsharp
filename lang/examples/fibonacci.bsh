#!include "../../code/std.bsh"


int fibonacci(int n) {
    if (n < 2) {
        return n
    }
    
    int a = 0;
    int b = 1;
    int i = 2;
    int temp = 0;
    
    while (i <= n) {
        temp = a + b;
        a = b;
        b = temp;
        i = i + 1;
    }
    return b;
}

int main(long argc, char **argv){
    int result = fibonacci(30)
    print("Final result: ")
    print(int_to_string(result))
    print("\n")
    exit(0)
    return 0
}
